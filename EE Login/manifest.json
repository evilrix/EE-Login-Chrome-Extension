{
   "name": "EE Login",
   "manifest_version": 2,
   "version": "1.0.4.0",
   "description": "Switch to your mod account at the click of a button",
   "background": {
      "scripts": [ "data.js", "background.js" ]
   },
   "page_action": {
      "default_icon": "icon16.png",
      "default_title": "EE Login"
   },
   "options_page": "options.html",
   "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
   "permissions": [
      "tabs"
      ]
}
